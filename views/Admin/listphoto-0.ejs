        <div class="content">
            <div class="container-fluid">               

                
                

                <div class="row">
                    <div class="col-md-12">
                        <div class="card">
                            <div class="header">
                                <div class="col-md-8">
                                    <h4 class="title">DANH SÁCH ẢNH CHƯA DUYỆT</h4>
                                    <p class="category">Cập nhật 3 phút trước</p>
                                    </div>
                                    <div class="col-md-4">                                                         
                                <p><input type="text"></input>
                                <button type="submit" class="btn btn-info btn-fill">Tìm kiếm</button>
                                </p>
                                </div>
                            </div>
                            <div class="content table-responsive table-full-width">
                                <table class="table table-hover table-striped">
                                    <thead>
                                        <th>ID</th>                                       
                                    	
                                    	<th>Title</th>
                                        <th>Id_user</th>
                                        <th>status_photo</th>
                                        <th>Images_description</th>
                                        <th>Images_url</th>
                                                                               
                                    </thead>
                                    <tbody>
                                       
                                        <% if(data && data.posts){%>
                                            <% for(var i=0;i< data.posts.length;i++){%>
                                            <tr>                                                
                                                
                                                <td><%= data.posts[i].id %></td>
                                                <td><%= data.posts[i].title %></td>
                                                <td><%= data.posts[i].id_user %></td>
                                                <td><%= data.posts[i].status_photo %></td>
                                                <td><%= data.posts[i].images_description %></td>
                                                <td><img src="../uploads/<%= data.posts[i].images_url %>"" style="width:250px;height:150px; object-fit: cover; "></td>                                                
                                                <td>
                                                    <a href="/admin/listduyetphoto0/<%=data.posts[i].id %>" class="btn btn-primary"> Duyệt </a>
                                                    <a href="/admin/listdeletephoto0/<%=data.posts[i].id %>" class="btn btn-danger">
                                                        Xóa </a>
                                                    
                                                </td>
                                            </tr>
                                            
                                            <%}%>
                                        <%}%>
                                                                               

                                        

                                    </tbody>
                                    
                                </table>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

